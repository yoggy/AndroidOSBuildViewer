<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout android:layout_width="fill_parent"
	android:layout_height="wrap_content" android:orientation="vertical"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/key"
		android:singleLine="true" android:gravity="left" android:textSize="12sp" android:textColor="#008800"/>
	/>
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/value"
		android:singleLine="true" android:gravity="left" android:textSize="20sp" />
</LinearLayout>
